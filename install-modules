puppet module install --force puppetlabs/git && puppet apply -e 'include git'
mv 'modules' 'modules.bak.20150501000303'
puppet module install --force --modulepath='modules' duxklr/manageusers
[ -e 'modules\server/.git/config' ] && cd 'modules\server' && git pull || (cd 'modules' && git clone https://github.com/mikegleasonjr/puppet-server server)
[ -e 'modules\gitlab/.git/config' ] && cd 'modules\gitlab' && git pull || (cd 'modules' && git clone https://github.com/spuder/puppet-gitlab gitlab)
cp -r 'modules-local/gitlab_cert' 'modules'
